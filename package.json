{
	"name": "videospeedup.com",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "tsx scripts/ensure-licenses-json.ts && vite dev",
		"build": "tsx scripts/ensure-licenses-json.ts && cross-env GENERATE_LICENSES=true vite build && vite build",
		"build:only": "vite build",
		"start": "tsx scripts/ensure-licenses-json.ts && vite build && vite preview",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"licenses:ensure": "tsx scripts/ensure-licenses-json.ts",
		"licenses:generate": "cross-env GENERATE_LICENSES=true vite build",
		"bunny:deploy": "tsx scripts/deploy-on-bunny.ts"
	},
	"devDependencies": {
		"@iconify-json/noto": "^1.1.18",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@tailwindcss/typography": "^0.5.10",
		"@types/html-minifier": "^4.0.5",
		"@types/node": "^20.10.6",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"cross-env": "^7.0.3",
		"daisyui": "^4.4.24",
		"dotenv": "^16.3.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"hasha": "^6.0.0",
		"html-minifier": "^4.0.0",
		"klaw": "^4.1.0",
		"modern-normalize": "^2.0.0",
		"pathe": "^1.1.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.10",
		"rollup-plugin-license": "^3.2.0",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.2",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"unplugin-icons": "^0.18.1",
		"vite": "^5.0.10"
	},
	"type": "module"
}
